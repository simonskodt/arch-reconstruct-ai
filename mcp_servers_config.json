{
  "context7": {
    "url": "https://mcp.context7.com/mcp",
    "transport": "streamable_http",
    "headers": {
      "Authorization": "Bearer CONTEXT7_API_KEY"
    }
  },
  "brave-search": {
    "transport": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "@brave/brave-search-mcp-server"
    ],
    "env": {
      "BRAVE_API_KEY": "BRAVE_API_KEY"
    }
  }
}
